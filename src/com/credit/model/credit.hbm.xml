<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.credit.model.CreditVO" table="EX_CREDIT" >
        
        <id name="cid" column="cid">
          <generator class="sequence" >
              <param name="sequence">ex_credit_seq</param>
            </generator>  
          </id>
     
        <property name="memberA_id" column="memberA_id" />
        <property name="memberB_id" column="memberB_id" />
        <property name="tid" column="tid" />
        <property name="status" column="status" />
     
    </class>
</hibernate-mapping>