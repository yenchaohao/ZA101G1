<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	
	<class name="com.report.model.ReportVO" table="ex_report">
			<id name="rid" column="rid">
				<generator class="sequence">
					<param name="sequence">ex_post_seq</param>
				</generator>
			</id>
	
			<property name="gid" column="gid" />
			<property name="member_id" column="member_id" />
			<property name="r_date" column="r_date" insert="false"/>
	</class>
	
</hibernate-mapping>